{
  "name": "musicavis",
  "version": "1.0.0",
  "description": "The Musicavis application provides musicians and aspiring musicians a platform for them to become more efficient with their practice sessions.",
  "main": "index.js",
  "repository": "https://github.com/reaper47/pymusical.git",
  "author": "Marc-Andr√© Charland",
  "license": "MIT",
  "scripts": {
    "it": "yarn install && python3 -m venv venv && . venv/bin/activate && python3 -m pip install -U pip && pip3 install -r requirements.txt",
    "rq": "rq worker musicavis-tasks",
    "it-pkgs": "rm -rf ./node_modules && yarn install",
    "build": "webpack --config webpack.config.js --progress --colors",
    "build-watch": "webpack --config webpack.config.js --progress --colors --watch"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bulma": "^0.9.1",
    "bulma-toast": "^2.2.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "date-fns": "^2.16.1",
    "howler": "^2.2.1",
    "table-dragger": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "^10.1.0",
    "webpack": "^5.11.0",
    "webpack-bundle-tracker": "^1.0.0-alpha.1",
    "webpack-cli": "^4.2.0"
  }
